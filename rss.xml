---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>gray.clhn.co: site updates</title>
    <link>http://gray.clhn.co/</link>
    <atom:link href="http://gray.clhn.co/rss.xml" rel="self" type="application/rss+xml" />
    <description>Updates to gray.clhn.co</description>
    <language>en-us</language>
    <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts %}
    {% unless post.categories contains '102' %}
    <item>
      {% if post.title %}<title>{{ post.title | xml_escape }}</title>{% endif %}
      <link>http://gray.clhn.co{{ post.url }}</link>
      <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
      <author>gray@clhn.co (Gray Calhoun)</author>
      <guid>http://gray.clhn.co{{ post.id }}</guid>
      <description>
	{{ post.content | xml_escape }}
      </description>
    </item>
    {% endunless %}{% endfor %}

  </channel>
</rss>